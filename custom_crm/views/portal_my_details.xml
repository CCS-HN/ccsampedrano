    <template id="portal_my_details_inherit_parents" >
        <t t-name="details_inherit_parents">
            <t t-call="portal.portal_layout">
                <t t-set="additional_title">parents Details</t>
                <!-- <h6>Vista parent</h6> -->
                <form action="/my/account/parents" method="post">
                    <input type="hidden" name="csrf_token" t-att-value="request.csrf_token()"/>
                    <div class="row o_portal_details">
                        <!-- <button id="page">Instalar</button> -->
                        <ul style ="padding-top: 15px; padding-bottom: 15px; width: 852px; list-style: none; display: inline; float: left; margin:auto" > 
                            <li t-if= "not part" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;"><div  id ="page" style ="width: 100px; color: white; display: block;background: #1919a5; border: 1px Solid #000; text-decoration: none;"  href='/my/account/parents'>Padre</div></li>
                            <li t-if= "part" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;"><div  id ="page" style ="width: 100px; color: white; display: block ;background: red; border: 1px Solid #000; text-decoration: none;"  href='/my/account/parents'>Padre</div></li>
                            

                            <li t-if= "not mother" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;" ><button type="submit" id ="page" style ="width: 100px; color: white; display: block;background: #1919a5; border: 1px Solid #000; text-decoration: none;" href='/my/account/motherM'>Madre</button></li>
                            <li t-if= "mother" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;" ><div id ="page" style ="width: 100px; color: white; display: block;background: red; border: 1px Solid #000; text-decoration: none;" href='/my/account/motherM'>Madre</div></li>
                            

                            <li t-if= "not encar" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;" ><div id ="page" style ="width: 100px; color: white; display: block;background: #1919a5; border: 1px Solid #000; text-decoration: none;" href='/my/account/encargado'>Encargado</div></li>
                            <li t-if= "encar" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;" ><div id ="page" style ="width: 100px; color: white; display: block;background: red; border: 1px Solid #000; text-decoration: none;" href='/my/account/encargado'>Encargado</div></li>
                            

                            <li t-if= "not materia" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;" ><div id ="page" style ="width: 100px; color: white; display: block;background: #1919a5; border: 1px Solid #000; text-decoration: none;" href='/my/account/materia'>Materia</div></li> 
                            <li t-if= "materia" style = "width: 100px; display: block; float: left; text-align: center; margin-right: 10px; position: relative; top: 1px;" ><div id ="page" style ="width: 100px; color: white; display: block;background: red; border: 1px Solid #000; text-decoration: none;" href='/my/account/materia'>Materia</div></li> 
                        
                        </ul>
                        
                        
                        <div class="col-lg-8">
                            <h4 style = "text-align:center">Padre</h4>   
                            <hr/>
                            <div class="row">
                                <div class="col-lg-12">
                                <div t-if="error_message" class="alert alert-danger" role="alert">
                                    <t t-foreach="error_message" t-as="err"><t t-esc="err"/><br /></t>
                                </div>
                                </div>
                                
                                <div t-attf-class="form-group #{error.get('id_padre') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Identidad</label>
                                    <input type="text" name="id_padre" t-attf-class="form-control #{error.get('id_padre') and 'is-invalid' or ''}" t-att-value="id_padre or partner.id_padre" />
                                </div>
                                <div t-attf-class="form-group #{error.get('father_name') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Nombre del Padre</label>
                                    <input type="text" name="father_name" t-attf-class="form-control #{error.get('father_name') and 'is-invalid' or ''}" t-att-value="father_name or partner.father_name" />
                                </div>

                                <div t-attf-class="form-group #{error.get('fecha_padre') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Fecha de nacimiento</label>
                                    <input type="date" name="fecha_padre" t-attf-class="form-control #{error.get('fecha_padre') and 'is-invalid' or ''}" t-att-value="fecha_padre or partner.fecha_padre" />
                                </div>
                                <div t-attf-class="form-group #{error.get('tel_padre') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Teléfono del trabajo</label>
                                    <input type="text" name="tel_padre" t-attf-class="form-control #{error.get('tel_padre') and 'is-invalid' or ''}" t-att-value="tel_padre or partner.tel_padre" />
                                </div>
                                <div t-attf-class="form-group #{error.get('correo_padre') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Correo del Padre</label>
                                    <input type="text" name="correo_padre" t-attf-class="form-control #{error.get('correo_padre') and 'is-invalid' or ''}" t-att-value="correo_padre or partner.correo_padre" />
                                </div>
                                <div t-attf-class="form-group #{error.get('movil_padre') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Móvil</label>
                                    <input type="text" name="movil_padre" t-attf-class="form-control #{error.get('movil_padre') and 'is-invalid' or ''}" t-att-value="movil_padre or partner.movil_padre" />
                                </div>
                                <div t-attf-class="form-group #{error.get('lugar_trb_padre') and 'o_has_error' or ''} col-xl-6">
                                    <label class="col-form-label label-optional" for="name">Lugar de trabajo</label>
                                    <input type="text" name="lugar_trb_padre" t-attf-class="form-control #{error.get('lugar_trb_padre') and 'is-invalid' or ''}" t-att-value="lugar_trb_padre or partner.lugar_trb_padre" />
                                </div>

                            
                                <input type="hidden" name="redirect" t-att-value="redirect"/>
                            </div>
                           
                            
                            <div class="clearfix">
                                <button type="submit" class="btn btn-primary float-right mb32 ">
                                    Siguiente
                                    <span class="fa fa-long-arrow-right" />
                                </button>
                            </div>
                        </div>
                    </div>
                </form>
            </t>
        </t>
    </template>

